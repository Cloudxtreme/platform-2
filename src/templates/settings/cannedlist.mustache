
		<div class="collapse-open">
			<div class="portlet box yellow">
				<div class="portlet-title">
					<div class="caption"><i class="fa fa-bar-chart"></i>Message templates</div>
					<div class="tools">
						<span class="collapse pull-right"></span>
					</div>
				</div>
				<div class="portlet-body">
					<ul class="campaign-list">
						
						{{#canned}}
							<li>
								<i class="yellow icon-bullseye"></i>
								<span class="campaign-name">{{attributes.body.plaintext}}</span>
								
								<span class="pull-right">
									<i class="fa fa-remove" data-delete-canned-id="{{id}}"></i>
								</span>
								
							</li>
						{{/canned}}
						
					</ul>
				</div>
			</div>
		</div>
