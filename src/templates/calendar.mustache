	<!-- Calendar pageview -->
	<div class="row-fluid">
		<h3 class="page-title"> </h3>
	</div>
	
	<div class="row-fluid stats-header">
		
		{{^customActive}}
		<span class="period-buttons btn-group">
			<a class="btn" id="subtract"><i class="icon-caret-left"></i></a>
			<a class="btn" id="add"><i class="icon-caret-right"></i></a>
		</span>
		{{/customActive}}
			
		<h3 class="stats-header-timeview"><i class="icon-cloud-download"></i></h3>
		
		<div class="pull-right">
			
			<a class="btn" id="now" class="hidden">{{translate_now}}</a>
			<a class="btn"><i class="icon-print"></i></a>
			<select>
				<optgroup label="Time spans">
					<option value="agendaDay" {{#datActive}}selected="true"{{/datActive}}>{{translate_day}}</option>
					<option value="agendaWeek" {{#weekActive}}selected="true"{{/weekActive}}>{{translate_week}}</option>
					<option value="month" {{#monthActive}}selected="true"{{/monthActive}}>{{translate_month}}</option>
					<!--<option value="list" {{#listActive}}selected="true"{{/listActive}}>{{translate_list_view}}</option>-->
				</optgroup>
			</select>

		</div>
	</div>
	
	<div id="widgetcontainer" class="row-fluid"></div>

	<div class="container-loading"><i class="icon-cloud-download"></i></div>

	<div id="calcontainer" class="row-fluid">
		
		<div id="calendar" class="has-no-toolbar"></div>
		
