<div class="row">
	<div class="span12">

		<div class="portlet box grey channelfilters">
			<div class="portlet-title line">
				<div class="caption"><i class="fa fa-briefcase icon-large"></i>{{#trans}}Company accounts{{/trans}}</div>
			</div>
			<div class="portlet-body">
				<div class="scroller">
				
					<div id="filter_streams" class="clearfix">
		
						<ul class="social-icon-colors">
						{{#streams}}
							<li data-streams="{{id}}" class="btn-white active"><i class="fa fa-{{icon}}"></i> {{name}}</option>
						{{/streams}}
							<li class="toggleall inactive">{{#trans}}select all{{/trans}}</li>
						</ul>
					
					</div>
					
				</div>
			</div>
		</div>

	</div>
</div>
