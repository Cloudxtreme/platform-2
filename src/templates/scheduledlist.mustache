<div class="portlet box blue messagelist" id="monitorlist">
	<div class="portlet-title line">
		<div class="caption"><i class="icon-time icon-large"></i>{{#trans}}Scheduled messages{{/trans}}</div>
		<div class="tools">
			<!-- <i class="icon-cloud-download"></i> -->
		</div>
	</div>

	<div class="portlet-body social-icon-colors toload">
		<div class="scroller">
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<td>{{#trans}}Start date{{/trans}}</td>
						<td>{{#trans}}Networks{{/trans}}</td>
						<td>{{#trans}}Message{{/trans}}</td>
						<td><small>{{#trans}}Actions{{/trans}}</small></td>
					</tr>
				</thead>
				<tbody class="messages-container"></tbody>
			</table>
		</div>
	</div>
</div>