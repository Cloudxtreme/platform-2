<div class="dashboard-stat {{network.icon}}-color">
	<div class="visual">
		<i class="icon-{{network.icon}}"></i>
	</div>

	{{#details}}
	<div class="details">
		<div class="number">
			{{content}}
		</div>
		<div class="desc">                           
			{{descr}}
		</div>
	</div>
	{{/details}}

	<a class="more" href="#">
		<strong>{{network.name}}</strong> {{title}} <i class="m-icon-swapright m-icon-white"></i>
	</a>
</div>