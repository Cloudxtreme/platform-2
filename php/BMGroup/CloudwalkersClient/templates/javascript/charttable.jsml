<table class="table table-striped table-bordered table-hover">

	<thead>
		<tr>
			{{#header}}
				<th>
					{{name}}
				</th>
			{{/header}}
		</tr>
	</thead>

	<tbody>
		{{#rows}}

			<tr>
				{{#columns}}
					<td>
						{{value}}
					</td>
				{{/columns}}
			</tr>

		{{/rows}}
	</tbody>

</table>