<div class="dashboard-stat {{network.icon}}-color">
	<div class="visual">
		<i class="icon-{{network.icon}}"></i>
	</div>

	<div class="details">
		<div class="number">
			{{details.content}}
		</div>
		<div class="desc">                           
			{{{details.title}}}
		</div>
	</div>

	<a class="more" href="#reports/{{streamid}}">
		{{{details.description}}}
		{{#dashboard}}<i class="icon-chevron-right m-icon-white"></i>{{/dashboard}}
	</a>
</div>