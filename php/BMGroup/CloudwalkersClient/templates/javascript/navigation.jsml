	<!-- Inbox -->
	{{#inbox}}
		<li>
			<a href="#inbox/messages">
				<i class="icon-inbox"></i> 
				<span class="title">{{translate_inbox}}</span>
				<span class="arrow"></span>
				<span class="selected"></span>
			</a>

			<ul class="sub-menu">

				{{#authorized.MESSAGE_READ_INBOX_MESSAGES}}
				<li>
					<a href="#inbox/messages">
						<i class="icon-envelope"></i> 
						<span class="title">{{translate_messages}}</span>
					</a>
				</li>
				{{/authorized.MESSAGE_READ_INBOX_MESSAGES}}
				{{#authorized.MESSAGE_READ_INBOX_NOTIFICATIONS}}
				<li>
					<a href="#inbox/notifications">
