				</li>
				{{/authorized.MESSAGE_READ_INBOX_NOTIFICATIONS}}

				<li>
					<a data-header-action="post">
						<i class="icon-pencil"></i> 
						<span class="title">{{translate_post_message}}</span>
					</a>
				</li>
				{{#authorized.MESSAGE_READ_DRAFTS}}
				<li class="drafts">
					<a href="#drafts">
						<i class="icon-edit"></i> 
						<span class="title">{{translate_drafts}}</span>
					</a>
				</li>
				{{/authorized.MESSAGE_READ_DRAFTS}}
				{{#authorized.MESSAGE_READ_SCHEDULE}}
				<li class="schedule">
					<a href="#scheduled">
						<i class="icon-time"></i> 
						<span class="title">{{translate_scheduled}}</span>
					</a>
				</li>
